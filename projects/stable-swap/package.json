{
    "name": "qube-stable-swap",
    "description": "Qube Stable Swap",
    "main": "index.js",
    "version": "1.0.0",
    "license": "MIT",
    "private": true,
    "scripts": {
        "compile": "npx hardhat compile",
		"deploy": "npx hardhat run --network mainnet scripts/deploy.ts",
		"deploy_LPFactory": "npx hardhat run --network mainnet scripts/deploy_LPFactory.ts",
		"deploy_swapTwoPoolDeployer": "npx hardhat run --network mainnet scripts/deploy_swapTwoPoolDeployer.ts",
		"deploy_swapThreePoolDeployer": "npx hardhat run --network mainnet scripts/deploy_swapThreePoolDeployer.ts",
		"deploy_TwoPoolInfo": "npx hardhat run --network mainnet scripts/deploy_TwoPoolInfo.ts",
		"deploy_ThreePoolInfo": "npx hardhat run --network mainnet scripts/deploy_ThreePoolInfo.ts",
		"deploy_SwapInfo": "npx hardhat run --network mainnet scripts/deploy_SwapInfo.ts",
		"flatten_Factory": "npx hardhat flatten contracts/QubeStableSwapFactory.sol > QubeStableSwapFactory_flat.sol",
		"flatten_LPFactory": "npx hardhat flatten contracts/QubeStableSwapLPFactory.sol > QubeStableSwapLPFactory_flat.sol",
		"flatten_swapTwoPoolDeployer": "npx hardhat flatten contracts/QubeStableSwapTwoPoolDeployer.sol > QubeStableSwapTwoPoolDeployer_flat.sol",
		"flatten_swapThreePoolDeployer": "npx hardhat flatten contracts/QubeStableSwapThreePoolDeployer.sol > QubeStableSwapThreePoolDeployer_flat.sol",
		"flatten_TwoPoolInfo": "npx hardhat flatten contracts/utils//QubeStableSwapTwoPoolInfo.sol > QubeStableSwapTwoPoolInfo_flat.sol",
		"flatten_ThreePoolInfo": "npx hardhat flatten contracts/utils//QubeStableSwapThreePoolInfo.sol > QubeStableSwapThreePoolInfo_flat.sol",
		"flatten_SwapInfo": "npx hardhat flatten contracts/utils//QubeStableSwapInfo.sol > QubeStableSwapInfo_flat.sol",
        "deploy:testnet": "npx hardhat run --network testnet scripts/deploy.ts",
        "deploy:mainnet": "npx hardhat run --network mainnet scripts/deploy.ts",
        "deploy_LPFactory:testnet": "npx hardhat run --network testnet scripts/deploy_LPFactory.ts",
        "deploy_LPFactory:mainnet": "npx hardhat run --network mainnet scripts/deploy_LPFactory.ts",
        "deploy_swapTwoPoolDeployer:testnet": "npx hardhat run --network testnet scripts/deploy_swapTwoPoolDeployer.ts",
        "deploy_swapTwoPoolDeployer:mainnet": "npx hardhat run --network mainnet scripts/deploy_swapTwoPoolDeployer.ts",
        "deploy_swapThreePoolDeployer:testnet": "npx hardhat run --network testnet scripts/deploy_swapThreePoolDeployer.ts",
        "deploy_swapThreePoolDeployer:mainnet": "npx hardhat run --network mainnet scripts/deploy_swapThreePoolDeployer.ts",
		"clean": "rm -rf node_modules",
		"reset": "rm -rf node_modules && rm -rf artifacts && rm -rf typechain-types && rm -rf abi && rm -rf cache",
        "test": "npx hardhat test",
        "test:coverage": "npx hardhat coverage",
        "test:size-contracts": "npx hardhat size-contracts"
    },
    "dependencies": {
        "@openzeppelin-4.5.0/contracts": "npm:@openzeppelin/contracts@4.5.0"
    }
}